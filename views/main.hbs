<div class="formsButton">
  <iframe class="video" width="550" height="347" src="https://www.youtube.com/embed/2Wnif4SR8P0"
    title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; 
    clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
</div>

<div class="container" style="width: 800px;">
  {{#each allTitle}}
  <div class="course">
    <div>{{this.title}}</div>
    {{#each this.lecturesId}}
    <div class="lecture"><a href="/courses/{{allTitle.title}}/lectures/{{this.title}}">{{this.title}}</a></div>
    {{/each}}
  </div>
  {{/each}}
</div>

